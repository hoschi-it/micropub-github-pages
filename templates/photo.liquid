---
layout: photo
{% if category -%}
tags:
{% for cat in category -%}
- {{ cat }}
{% endfor -%}
{% endif -%}
{% if slug -%}
permalink: "{{ slug }}"
{% endif -%}
date: {{ published }}
{% if fm_published -%}
published: {{ fm_published }}
{% endif -%}
{% if syndication -%}
syndication:
{% for url in syndication -%}
- {{ url }}
{% endfor -%}
{% endif -%}
---
{% if photo -%}
  {% for pic in photo -%}
![{{ pic["alt"] }}]({{ pic["url"] }}){: .u-photo}
  {% endfor -%}
{% endif %}
{{ content }}
